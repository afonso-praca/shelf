{
  "resourceBindings": [
    {
      "locator": "search@vtex.storefront-sdk",
      "relativePath": "/{{ account }}/products",
      "queryParams": {
        "category": "{{ settings.category }}",
        "collection": "{{ settings.collection }}",
        "pageSize": "{{ settings.quantity }}",
        "availableOnly": "{{ true }}"
      },
      "bindTo": "products@vtex"
    }
  ],
  "assets": [
    "Shelf.js",
    "editors/index.js"
  ],
  "implements": ["HomePage@vtex.storefront-theme#shelf"]
}
